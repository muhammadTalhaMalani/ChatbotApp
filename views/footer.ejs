<script>
    $(document).ready(() => {

                 
        var UserID = parseInt($('#UserID').val());

        if(UserID === 1 || UserID === 2){
            $('#qrcode, #client_mapping').removeClass('d-none');
        }
    });

    function logout() {

        $.ajax({
            url: '/logout',
            method: 'GET',
            success: osLogout,
            error: (err) => {
                console.log(err);
            }
        });

    }

    function osLogout(res) {

        if (res === true) {
            window.location = '/';
        } else {

            new Noty({
                type: 'error',
                layout: 'topRight',
                theme: 'bootstrap-v4',
                text: 'Something Went Wrong',
                timeout: 3000
            }).show();

        }

    }
</script>
